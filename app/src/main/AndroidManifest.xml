<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fcu.mp110.food_delivery_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Nav4.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.restaurant.CategoriesResultActivity"
            android:exported="false" />
        <activity
            android:name=".ui.store.OrderDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ui.store.CheckOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.order.OrderStatusActivity"
            android:exported="false" />
        <activity
            android:name=".ui.restaurant.CartActivity"
            android:exported="false" />
        <activity
            android:name=".ui.review.ReviewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.restaurant.FoodDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.search.SearchResultPage"
            android:exported="false" />
        <activity
            android:name=".ui.search.SearchPage"
            android:exported="false" />
        <activity
            android:name=".ui.restaurant.RestaurantMenu"
            android:exported="false" />
        <activity
            android:name=".ui.store.StoreActivity"
            android:exported="true" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.Login2Activity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Nav4.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>